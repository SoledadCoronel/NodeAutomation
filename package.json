{
  "name": "node_api_test",
  "version": "0.1.0",
  "description": "Package json for api tests",
  "main": "login.js",
  "private": true,
  "scripts": {
    "datafixture": "babel-node test/datafixture.js --presets es2015",
    "test": "mocha \"test/**/*@(.js)\" --compilers js:babel-register --recursive -R xunit test/ > test-reports.xml --timeout 45000",
    "singletest": "clear && mocha \"test/platforms/testsGalleries@(.js)\" --compilers js:babel-register --timeout 40000",
    "singletest2": "clear && mocha \"test/users/testRole*@(.js)\" --compilers js:babel-register"
  },
  "keywords": [
    "node.js",
    "eclipse",
    "nodeclipse"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chai": "^3.5.0",
    "chai-colors": "^1.0.1",
    "chai-http": "^3.0.0",
    "jsonapi-serializer": "^3.4.1",
    "jsonfile": "^2.4.0",
    "mocha": "^3.0.2",
    "mocha-junit-reporter": "^1.12.1",
    "random-js": "^1.0.8"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0"
  }
}
